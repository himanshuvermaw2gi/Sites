{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "riyer"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "ram@w2gi"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
	{
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div > b"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"text\"]"
      },
      "text": "fellowes"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "em"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "change_account"
      }
    },
	{
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Etailer\"]"
      }
    },
	{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Providers"
      }
    },
	{
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
