{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "riyer"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "ram@w2gi"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
	{
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div > b"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"text\"]"
      },
      "text": "fellowes"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "em"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "change_account"
      }
    },
	{
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Etailer\"]"
      }
    },
	{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Configuration"
      }
    },
	{
      "type": "pause",
      "waitTime": "6000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "span.icon-search"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input.search-box"
      },
      "text": "wafflehouse"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.Button.btn-search"
      }
    },
	{
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "verifyTextPresent",
      "text": "Data providers - Account"
    },
	{
      "type": "pause",
      "waitTime": "4000"
    },
    {
      "type": "verifyTextPresent",
      "text": "No data found"
    }
  ]
}
