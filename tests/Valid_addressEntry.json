{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/index-nativemap.html"
    },
{
        "type": "pause",
        "waitTime": "5000"
        },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "MountainView Hospital-Las Vegas, NV"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
{
        "type": "pause",
        "waitTime": "7000"
        },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "5469 Mountain Vista St Las Vegas, NV 89120"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='locator_search']/input[2]"
      }
    },
    
{
        "type": "pause",
        "waitTime": "20000"
        },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.getdirections"
      }
    }
  ]
}
