{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/index-nativemap.html"
    },
	
	{
	"type": "pause",
	"waitTime": "5000"
	},
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "Denver , 80201 thru 80239"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "body"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
	{
	"type": "pause",
	"waitTime": "8000"
	},
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Get Directions"
      }
    }
  ]
}
