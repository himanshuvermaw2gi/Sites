{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://mobile.where2stageit.com/ramtest/index-nativemap.html"
    },
{
	"type": "pause",
	"waitTime": "4000"
	}

    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "addressline"
      },
      "text": "rajendra nagar, New Delhi"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
        "type": "pause",
        "waitTime": "7000"
        },
    {
      "type": "verifyTextPresent",
      "text": "No locations were found using your search criteria [ rajendra nagar, New Delhi US ]. Please try another input address to search for locations."
    },
    {
        "type": "pause",
        "waitTime": "5000"
        }
  ]
}
