{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": ""
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
	 {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div > b"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"text\"]"
      },
      "text": "dennys"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "#accounts_chzn_o_78 > em"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "change_account"
      }
    },
	 {
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Launchpad\"]"
      }
    },
	{
      "type": "pause",
      "waitTime": "3000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "date-range"
      }
    },
    {
      "type": "pause",
      "waitTime": "10000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "a.datepickerGoPrev > span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='datepickerContainer']//span[.='1']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='datepickerContainer']/table/tbody/tr/td[5]/table/tbody[2]/tr[5]/td[5]/a/span"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.datepickerApply.GrayButton"
      }
    },
    {
      "type": "pause",
      "waitTime": "10000"
    }
  ]
}
